{
  "version": 2,
  "name": "painaidee-3d-map",
  "builds": [
    {
      "src": "src/pages/home/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/assets/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/src/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/src/pages/home/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self' https:; script-src 'self' 'unsafe-inline' https://unpkg.com https://fonts.googleapis.com; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com https://unpkg.com; font-src 'self' https://fonts.gstatic.com; img-src 'self' data: https:; connect-src 'self' https:;"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/src/pages/home/index.html"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/src/pages/home/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "public": false,
  "regions": ["sin1", "hnd1", "sfo1"],
  "framework": null,
  "buildCommand": "npm run build:production",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run serve:dev",
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "functions": {},
  "github": {
    "autoAlias": true
  }
}
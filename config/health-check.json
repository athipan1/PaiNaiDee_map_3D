    },
    {
      "name": "CSS Assets",
      "url": "/assets/css/styles.css",
      "method": "GET",
      "expectedStatus": 200,
      "expectedContentType": "text/css",
      "timeout": 3000
    },
    {
      "name": "JavaScript Assets",
      "url": "/assets/js/script.js",
      "method": "GET",
      "expectedStatus": 200,
      "expectedContentType": "application/javascript",
      "timeout": 3000
    }
  ],
  "monitoring": {
    "interval": "5m",
    "retries": 3,
    "alerting": {
      "enabled": true,
      "channels": ["email", "slack"]
    }
  },
  "performance": {
    "lighthouse": {
      "performance": ">= 90",
      "accessibility": ">= 95",
      "bestPractices": ">= 90",
